# ============================================
# RESEARCH & DATA TOOLS ‚úÖ
# ============================================

# Tavily Research API
TAVILY_API_KEY=tvly-your-tavily-api-key-here

# Firecrawl Web Scraping
FIRECRAWL_API_KEY=fc-your-firecrawl-api-key-here

# ============================================
# FINANCIAL DATA PROVIDERS ‚úÖ
# ============================================

# üÜì FREE TIER PROVIDERS (No keys required - 100% FREE)
# Yahoo Finance - Completely FREE unlimited (no API key needed - uses yfinance library)
# SEC Edgar - US Government data (requires user agent only)
SEC_EDGAR_USER_AGENT="Axiom_Investment_Banking your_email@domain.com"

# üí∞ AFFORDABLE PREMIUM FINANCIAL DATA PROVIDERS

# Alpha Vantage - Use comma-separated keys for rotation
# FREE tier: 500 calls/day per key
ALPHA_VANTAGE_API_KEY=key1,key2,key3,key4,key5,key6

# Financial Modeling Prep (FMP)
# FREE tier: 250 calls/day | Premium: $14/month
FINANCIAL_MODELING_PREP_API_KEY=your-fmp-api-key-here
FMP_API_KEY=your-fmp-api-key-here

# Finnhub
# FREE tier: 60 calls/minute | Premium: $7.99/month
FINNHUB_API_KEY=your-finnhub-api-key-here

# üèÜ PROFESSIONAL MCP SERVERS API KEYS

# Official Polygon.io MCP Server
# FREE tier: 5 calls/minute | Premium: $25/month
POLYGON_API_KEY=your-polygon-api-key-here

# üîÑ API ROTATION CONFIGURATION
FINANCIAL_API_ROTATION_ENABLED=true
ALPHA_VANTAGE_API_ROTATION_ENABLED=true
POLYGON_API_ROTATION_ENABLED=true
FINNHUB_API_ROTATION_ENABLED=true
FMP_API_ROTATION_ENABLED=true

# ============================================
# DATABASE CONFIGURATION
# ============================================

# PostgreSQL (Primary Database)
POSTGRES_USER=axiom
POSTGRES_PASSWORD=axiom_password
POSTGRES_DB=axiom_finance
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
DATABASE_URL=postgresql://axiom:axiom_password@localhost:5432/axiom_finance

# Redis Cache
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=axiom_redis
REDIS_URL=redis://:axiom_redis@localhost:6379

# Vector Database (ChromaDB)
CHROMA_HOST=localhost
CHROMA_PORT=8000
CHROMA_URL=http://localhost:8000

# Graph Database (Neo4j)
NEO4J_USER=neo4j
NEO4J_PASSWORD=axiom_neo4j
NEO4J_BOLT_PORT=7687
NEO4J_HTTP_PORT=7474
NEO4J_URI=bolt://localhost:7687

# ============================================
# LANGCHAIN TRACING & OBSERVABILITY ‚úÖ
# ============================================

LANGCHAIN_TRACING_V2=true
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com
LANGCHAIN_API_KEY=lsv2_pt_your-langsmith-api-key-here
LANGCHAIN_PROJECT=axiom-investment-banking

# ============================================
# MLFLOW EXPERIMENT TRACKING
# ============================================

MLFLOW_TRACKING_URI=http://localhost:5000
MLFLOW_ARTIFACT_ROOT=./mlflow/artifacts
MLFLOW_EXPERIMENT_NAME=axiom-quant-finance

# ============================================
# FEAST FEATURE STORE
# ============================================

FEAST_REGISTRY_PATH=./feature_repo/
FEAST_ONLINE_STORE=redis://localhost:6379

# ============================================
# GPU CONFIGURATION (RTX 4090)
# ============================================

# Configure for your GPU (RTX 4090 has ~15GB VRAM)
ENABLE_GPU=true
CUDA_VISIBLE_DEVICES=0
PYTORCH_ALLOC_CONF=max_split_size_mb:512

# ============================================
# INVESTMENT BANKING ANALYSIS SETTINGS
# ============================================

DEBUG=false
MAX_PARALLEL_ANALYSIS_TASKS=5
FINANCIAL_DATA_DEPTH=comprehensive
DUE_DILIGENCE_CONFIDENCE_THRESHOLD=0.65
VALUATION_MODEL_TYPES=dcf,comparable,precedent

# Risk Assessment Parameters
RISK_ANALYSIS_ENABLED=true
REGULATORY_COMPLIANCE_CHECK=true
MARKET_VOLATILITY_ASSESSMENT=true

# ============================================
# API CONFIGURATION
# ============================================

API_HOST=0.0.0.0
API_PORT=8000
API_SECRET_KEY=axiom-secret-key-change-in-production
CORS_ORIGINS=http://localhost:3000,http://localhost:8000

# ============================================
# MODEL CONFIGURATION
# ============================================

MODEL_CACHE_SIZE=25
BATCH_SIZE=32
MAX_WORKERS=4
DEFAULT_MODEL_PROVIDER=claude
DEFAULT_MODEL=claude-3-5-sonnet-20241022

# ============================================
# PERFORMANCE TUNING
# ============================================

ENABLE_MODEL_CACHING=true
ENABLE_RESPONSE_CACHING=true
CACHE_TTL_SECONDS=3600
CONNECTION_POOL_SIZE=10
MAX_OVERFLOW_CONNECTIONS=20

# ============================================
# MONITORING & OBSERVABILITY
# ============================================

PROMETHEUS_ENABLED=true
PROMETHEUS_PORT=9090
GRAFANA_ENABLED=true
GRAFANA_PORT=3000

# ============================================
# SECURITY
# ============================================

JWT_SECRET_KEY=axiom-jwt-secret-change-in-production
API_KEY_SALT=axiom-api-salt-change-in-production
ENABLE_API_KEY_AUTH=true
ENABLE_RATE_LIMITING=true
RATE_LIMIT_PER_MINUTE=60

# ============================================
# LOGGING
# ============================================

LOG_LEVEL=INFO
LOG_FORMAT=json
LOG_FILE=./logs/axiom.log
ENABLE_FILE_LOGGING=true
ENABLE_CONSOLE_LOGGING=true

# ============================================
# FEATURE FLAGS
# ============================================

ENABLE_EXPERIMENTAL_FEATURES=false
ENABLE_DEBUG_MODE=false
ENABLE_PROFILING=false

# ============================================
# MCP SERVERS
# ============================================

MCP_SERVER_HOST=localhost
MCP_SERVER_PORT_START=8100
ENABLE_MCP_SERVERS=true

# ============================================
# DERIVATIVES PLATFORM
# ============================================

ENABLE_DERIVATIVES=true
MAX_GREEKS_BATCH_SIZE=1000
VOLATILITY_SURFACE_CACHE_TTL=300

# ============================================
# DEVELOPMENT SETTINGS
# ============================================

TESTING=false
ENVIRONMENT=production

# ============================================
# PATHS
# ============================================

PROJECT_ROOT=/home/user/axiom
DATA_DIR=./data
OUTPUT_DIR=./outputs
MODEL_DIR=./models
CACHE_DIR=./.cache

# ============================================
# DEEP INTELLIGENCE WORKFLOWS
# ============================================

# Mode Configuration
DEEP_INTELLIGENCE_MODE=production
DEEP_INTELLIGENCE_ENABLED=true

# Workflow Enablement
SEC_PARSER_ENABLED=true
EARNINGS_ANALYZER_ENABLED=true
ALT_DATA_SYNTHESIZER_ENABLED=true

# Claude Settings for Deep Intelligence
MAX_TOKENS=8192
TEMPERATURE=0.1

# Analysis Depth Settings
EXTRACT_ALL_RISK_FACTORS=true
TRACK_RISK_CHANGES=true
ANALYZE_FOOTNOTES=true
EXTRACT_LEGAL_PROCEEDINGS=true
QUANTIFY_SUPPLIER_DEPENDENCIES=true

# Earnings Call Analysis
EARNINGS_QUARTERS_TO_ANALYZE=40
MANAGEMENT_TONE_SCORING=true
EARLY_WARNING_DETECTION=true
STRATEGIC_PIVOT_TRACKING=true

# Alternative Data Sources
ALT_DATA_SOURCES=job_postings,patents,app_store,social,web_traffic,employee_reviews
LEADING_INDICATOR_TRACKING=true
PREDICTION_LEAD_TIME_MONTHS=12

# Output Settings
GENERATE_INVESTMENT_THESIS=true
BLOOMBERG_COMPARISON_MODE=true
ENABLE_COST_TRACKING=true