{
  "dashboard": {
    "title": "AI/ML Platform Overview",
    "tags": ["ai", "ml", "platform", "overview"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Platform Health Score",
        "type": "gauge",
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0},
        "targets": [{
          "expr": "avg(system_health_score)",
          "refId": "A"
        }],
        "options": {
          "showThresholdLabels": false,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 0.7, "color": "yellow"},
                {"value": 0.9, "color": "green"}
              ]
            },
            "unit": "percentunit"
          }
        }
      },
      {
        "id": 2,
        "title": "Claude API Cost (Today)",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0},
        "targets": [{
          "expr": "claude_api_cost_daily_usd",
          "refId": "A"
        }],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 100, "color": "yellow"},
                {"value": 500, "color": "red"}
              ]
            },
            "unit": "currencyUSD"
          }
        }
      },
      {
        "id": 3,
        "title": "Active DAGs",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
        "targets": [{
          "expr": "count(count by (dag_id) (airflow_dag_runs_total))",
          "refId": "A"
        }],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 1, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Data Quality Score (Average)",
        "type": "gauge",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0},
        "targets": [{
          "expr": "avg(data_quality_score)",
          "refId": "A"
        }],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 60, "color": "yellow"},
                {"value": 80, "color": "green"}
              ]
            },
            "unit": "percent"
          }
        }
      },
      {
        "id": 5,
        "title": "AI Agent Execution Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [{
          "expr": "rate(langgraph_agent_executions_total[5m])",
          "legendFormat": "{{agent_name}} - {{status}}",
          "refId": "A"
        }],
        "yaxes": [
          {"format": "ops", "label": "Executions/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 6,
        "title": "DAG Success Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "rate(airflow_dag_runs_total{state=\"success\"}[5m]) / rate(airflow_dag_runs_total[5m])",
            "legendFormat": "{{dag_id}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percentunit", "label": "Success Rate", "min": 0, "max": 1},
          {"format": "short"}
        ]
      },
      {
        "id": 7,
        "title": "Claude API Calls (Rate)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [{
          "expr": "rate(claude_api_calls_total[5m])",
          "legendFormat": "{{model}} - {{cache_hit}}",
          "refId": "A"
        }],
        "yaxes": [
          {"format": "cps", "label": "Calls/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 8,
        "title": "Data Freshness by Table",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "targets": [{
          "expr": "data_freshness_minutes",
          "legendFormat": "{{table_name}}",
          "refId": "A"
        }],
        "yaxes": [
          {"format": "m", "label": "Minutes", "logBase": 1},
          {"format": "short"}
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [120], "type": "gt"},
              "operator": {"type": "and"},
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ],
          "frequency": "1m",
          "handler": 1,
          "name": "Data Freshness Alert",
          "notifications": []
        }
      },
      {
        "id": 9,
        "title": "Active Alerts",
        "type": "table",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 24},
        "targets": [{
          "expr": "ALERTS{alertstate=\"firing\"}",
          "format": "table",
          "instant": true,
          "refId": "A"
        }],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {},
              "indexByName": {},
              "renameByName": {
                "alertname": "Alert",
                "severity": "Severity",
                "summary": "Summary"
              }
            }
          }
        ]
      }
    ]
  }
}